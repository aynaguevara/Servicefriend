          "rotation": "random"
        }
 },
	
 "approval": {
        "output": {
          "type": "ogm",
          "text": ":-)"
        }
 },
	
 "denial": {
        "output": {
          "type": "ogm",
          "text": "ok"
        }
 },
	
 "typo": {
        "output": {
          "type": "ogm",
          "text": "Got it!"
        }
 },
	
 "disappointment": {
        "output": {
          "type": "ogm",
          "text": ["I'm sorry to hear that",":-("]
        }
 },
		
 "strong_approval": {
        "output": {
          "type": "ogm",
          "text": "Thank you very much!"
        }
 },
		
 "Im_here": {
        "output": {
          "type": "ogm",
          "text": "good to have you here"
        }
 },
	
    "thanks": {
        "output": {
          "type": "ogm",
          "text": "Sure thing!"
        }
    },
    "one_moment": {
        "output": {
          "type": "ogm",
          "text": "Sure, no problem, I'm always here"
        }
    },
    "have_q": {
